{
	"name": "latent-deploy",
	"version": "0.0.1-development",
	"private": false,
	"bin": {
		"latent": "build/index.js"
	},
	"scripts": {
		"build": "rm -rf build && tsc && cp -r src/*.hbs build/ && chmod +x build/index.js",
		"format": "prettier --single-quote --write './**/*'",
		"format:check": "prettier --single-quote --check './**/*'",
		"lint": "eslint \"./**/*.{js,ts,tsx,jsx}\" --quiet --fix",
		"lint:check": "eslint \"./**/*.{js,ts,tsx,jsx}\" --quiet",
		"check": "yarn run format:check && yarn run lint:check",
		"check:fix": "yarn run format && yarn run lint",
		"semantic-release": "semantic-release --branches main"
	},
	"dependencies": {
		"better-logging": "^4.5.0",
		"commander": "^8.1.0",
		"expand-tilde": "^2.0.2",
		"handlebars": "^4.7.7",
		"inquirer": "^8.1.2",
		"nanoid": "^3.1.23",
		"node-ssh": "^12.0.0"
	},
	"devDependencies": {
		"@types/commander": "^2.12.2",
		"@types/expand-tilde": "^2.0.0",
		"@types/node": "^14.14.6",
		"@types/react": "^16.9.55",
		"@typescript-eslint/eslint-plugin": "^4.1.0",
		"@typescript-eslint/parser": "^4.1.0",
		"eslint": "^7.9.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-config-wesbos": "^1.0.0",
		"eslint-plugin-html": "^6.1.0",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "7.18.0",
		"eslint-plugin-react-hooks": "1.7.0",
		"husky": "^4.3.0",
		"prettier": "^2.1.1",
		"prettier-eslint": "^11.0.0",
		"semantic-release": "^17.4.4",
		"typescript": "^4.0.5"
	},
	"husky": {
		"hooks": {
			"pre-push": "yarn run check:fix"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rishiosaur/latent.git"
	},
	"release": {
		"branches": [
			"master"
		]
	}
}
